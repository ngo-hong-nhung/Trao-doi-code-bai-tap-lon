    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         if (id.getText().equals("Type your Username here!") || pass.getText().equals("")) {
//Hiển thị thông báo bằng Message Dialog
JOptionPane.showMessageDialog(null, "Bạn chưa nhập UserName hoặc Password!", "Lỗi đăng nhập", JOptionPane.OK_OPTION);
}
      if (id.getText().equals(USER) && pass.getText().equals(PASS)) {
JOptionPane.showMessageDialog(null, "Logged", "Informations", JOptionPane.INFORMATION_MESSAGE);
 Trangchu c=new Trangchu();
 c.setVisible(true);
} else {
JOptionPane.showMessageDialog(null, "Log failure", "Informations", JOptionPane.OK_OPTION);
}  
       
    }                                        

